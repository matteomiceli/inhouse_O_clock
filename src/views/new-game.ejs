<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/style.css" rel="stylesheet" type="text/css">
    <title>Create Game</title>
</head>
<body>
    <nav class="navbar">

    </nav>
    <div class="container">
        <div class="game-bracket">
            <table class="game-table">
                <thead>
                    <tr>
                        <th>Red Team</th>
                        <th></th>
                        <th>Position</th>
                        <th></th>
                        <th>Blue Team</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>
                            <select name="cars" id="red-top">
                                <% for(i=0; i < players.length; i++) { %>
                            
                                    <option value='<%= players[i].alias %>'><%= players[i].alias %></option>
                                    
                                <% } %>
                             </select>
                        </td>
                        <td id="red-top-score">
                            
                        </td>
                        <td>
                            Top
                        </td>
                        <td class="blue-top-score">

                        </td>
                        <td>
                            <select name="players" id="blue-top">
                                <% for(i=0; i < players.length; i++) { %>
                            
                                    <option value='<%= players[i].alias %>'><%= players[i].alias %></option>
                                    
                                <% } %>
                             </select>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <select name="cars" id="red-jung">
                                <% for(i=0; i < players.length; i++) { %>
                            
                                    <option value='<%= players[i].alias %>'><%= players[i].alias %></option>
                                    
                                <% } %>
                             </select>
                        </td>
                        <td id="red-jung-score">
                            
                        </td>
                        <td>
                            jung
                        </td>

                        <td class="blue-jung-score">

                        </td>
                        <td>
                            <select name="cars" id="blue-jung">
                                <% for(i=0; i < players.length; i++) { %>
                            
                                    <option value='<%= players[i].alias %>'><%= players[i].alias %></option>
                                    
                                <% } %>
                             </select>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <select name="cars" id="red-mid">
                                <% for(i=0; i < players.length; i++) { %>
                            
                                    <option value='<%= players[i].alias %>'><%= players[i].alias %></option>
                                    
                                <% } %>
                             </select>
                        </td>
                        <td id="red-mid-score">
                            
                        </td>
                        <td>
                            mid
                        </td>

                        <td class="blue-mid-score">

                        </td>
                        <td>
                            <select name="cars" id="blue-mid">
                                <% for(i=0; i < players.length; i++) { %>
                            
                                    <option value='<%= players[i].alias %>'><%= players[i].alias %></option>
                                    
                                <% } %>
                             </select>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <select name="cars" id="red-adc">
                                <% for(i=0; i < players.length; i++) { %>
                            
                                    <option value='<%= players[i].alias %>'><%= players[i].alias %></option>
                                    
                                <% } %>
                             </select>
                        </td>
                        <td id="red-adc-score">
                            
                        </td>
                        <td>
                            adc
                        </td>

                        <td class="blue-adc-score">

                        </td>
                        <td>
                            <select name="cars" id="blue-adc">
                                <% for(i=0; i < players.length; i++) { %>
                            
                                    <option value='<%= players[i].alias %>'><%= players[i].alias %></option>
                                    
                                <% } %>
                             </select>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <select name="cars" id="red-sup">
                                <% for(i=0; i < players.length; i++) { %>
                            
                                    <option value='<%= players[i].alias %>'><%= players[i].alias %></option>
                                    
                                <% } %>
                             </select>
                        </td>
                        <td id="red-sup-score">
                            
                        </td>
                        <td>
                            sup
                        </td>
                        <td class="blue-sup-score">

                        </td>
                        <td>
                            <select name="cars" id="blue-sup">
                                <% for(i=0; i < players.length; i++) { %>
                            
                                    <option value='<%= players[i].alias %>'><%= players[i].alias %></option>
                                    
                                <% } %>
                             </select>
                        </td>
                    </tr>
                </tbody>
                
            </table>
        </div>
        <br>
        <br>
        <div class="player-pool">
            <table class="player-pool-table">
                <thead>
                    <tr>
                        <th>Summoner</th>
                        <th>Top</th>
                        <th>Jungle</th>
                        <th>Mid</th>
                        <th>ADC</th>
                        <th>Support</th>
                    </tr>
                </thead>
                <tbody>
                    <% for(i=0; i < players.length; i++) { %>
                        <tr>
                          <td><%= players[i].alias %></td>
                          <td><%= players[i].posRatings.top %></td>
                          <td><%= players[i].posRatings.jung %></td>
                          <td><%= players[i].posRatings.mid %></td>
                          <td><%= players[i].posRatings.adc%></td>
                          <td><%= players[i].posRatings.sup %></td>
                        </tr>
                    <% } %>
                </tbody>
                
            </table>
        </div>
    </div>
    <style src=""></style>
</body>
</html>